<div class="col-3">
    <a onclick="history.back()" type="button" class="btn text-white align-self-center"
        style="background-color: #563d7c ">
        <i class="bi bi-backspace-fill"></i> Go Back!</a>
</div>
<br>
{{#if alert}}
<div class="alert alert-success alert-dismissible fade show" role="alert">
    {{alert}}
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
{{/if}}

{{#each ans}}
<form class="row g-3 needs-validation" method="POST" action="/mobile/editmobile/{{this.Mob_ID}}">
    {{> mobile-form/mobile-edit-form }}
</form>
{{/each}}